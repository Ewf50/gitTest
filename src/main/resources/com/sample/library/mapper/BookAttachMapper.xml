  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sample.library.mapper.BookAttachMapper">
	<select id="getAttachesByUuids" resultType="BookAttachVO">
        <![CDATA[
		SELECT *
		FROM book_attach
		WHERE uuid IN
		]]>
        <foreach collection="list" item="uuid" open="(" separator="," close=")">
            #{uuid}
        </foreach>
    </select>
</mapper>